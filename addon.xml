<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.video.hun-tvs" name="Hungarian TV channels" version="0.0.2" provider-name="ivanjoe">
  <requires>
    <import addon="xbmc.python" version="2.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="addon.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Hungarian TV channels</summary>
      <langugage>en</langugage>
    <description lang="en">XBMC video addon for Hungarian TV channels:
    M1, M2, DunaTV, DunaWorld, ATV</description>
    <disclaimer lang="en">Feel free to use this script. For information visit the wiki.</disclaimer>
    <platform>all</platform>
    <nofanart>false</nofanart>
    <source>https://github.com/ivanjoe/xbmc-hun-tvs.git</source>
  </extension>
</addon>
